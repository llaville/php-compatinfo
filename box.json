{
    "alias": "phpcompatinfo.phar",
    "chmod": "0755",
    "directories": ["src"],
    "extract": true,
    "compression": "GZ",
    "files": [
        "LICENSE"
    ],
    "finder": [
        {
            "name": "*.php",
            "in": "vendor/nikic/php-parser/lib"
        },
        {
            "name": "*.php",
            "notPath": "/Tests/",
            "in": "vendor/symfony"
        },
        {
            "name": "*.php",
            "in": "vendor/phpunit/php-timer"
        },
        {
            "name": "*.php",
            "in": "vendor/bartlett/php-reflect/src"
        }
    ],
    "metadata": [
        {
            "vendor": [
                {
                    "nikic": [
                        {
                            "PHP-Parser": "1.0.0-beta1"
                        }
                    ],
                    "symfony": [
                        {
                            "classloader": "2.4.3",
                            "console": "2.4.3",
                            "eventdispatcher": "2.4.3",
                            "finder": "2.4.3"
                        }
                    ],
                    "phpunit": [
                        {
                            "php-timer": "1.0.5"
                        }
                    ],
                    "llaville": [
                        {
                            "php-reflect": "2.0.0"
                        }
                    ]
                }
            ]
        }
    ],
    "git-commit": "git_commit",
    "git-version": "package_version",
    "main": "bin/compatinfo",
    "output": "phpcompatinfo-@git-version@.phar",
    "intercept": true,
    "stub": true,
    "web": false
}
